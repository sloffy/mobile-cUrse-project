{
  "expo": {
    "name": "Лингвистический анализ текстов",
    "slug": "text-linguistic-analysis",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "icon": "./assets/icon.png",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.textanalysis.linguistic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.textanalysis.linguistic"
    },
    "extra": {
      "eas": {
        "projectId": "7f084676-670d-439f-bfd7-ca3186a27290"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
